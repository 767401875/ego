<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo 
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
   
   <!-- 添加服务消费者的标志 -->
   <dubbo:application name="ego-portal-web-consumer"/>
   <!-- 指定注册中心 -->
   <dubbo:registry address="106.12.34.249:2181,106.12.34.249:2182,106.12.34.249:2183" protocol="zookeeper" />
   <dubbo:reference interface="com.qsq.ego.rpc.service.ItemCatService" id="itemCatServiceProxy"></dubbo:reference>
   <dubbo:reference interface="com.qsq.ego.rpc.service.TbContentService" id="tbContentServiceProxy"></dubbo:reference>

</beans>
